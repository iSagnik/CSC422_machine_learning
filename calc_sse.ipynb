   "source": [
    "import numpy as np\n",
    "\n",
    "def sse(clusters, centroid):\n",
    "    sse = 0\n",
    "    for point in clusters:\n",
    "        sse += np.linalg.norm(centroid - point) ** 2\n",
    "    return sse\n",
    "\n",
    "clusters = np.array([\n",
    "    [5,8],\n",
    "    [7,9],\n",
    "])\n",
    "centroid = np.array((6, 8.5))\n",
    "\n",
    "print(sse(clusters, centroid))\n"
   ]
  }
